{
    "name": "pzh",
    "version": "2.0.0",
    "private": true,
    "homepage": "https://github.com/Provincie-Zuid-Holland/Omgevingsbeleid-Frontend",
    "type": "module",
    "dependencies": {
        "@headlessui/react": "^1.7.17",
        "@pzh-ui/components": "^0.0.580",
        "@pzh-ui/config": "^0.0.70",
        "@pzh-ui/css": "^0.0.100",
        "@pzh-ui/icons": "^0.0.69",
        "@react-hookz/web": "^24.0.4",
        "@tanstack/react-query": "^5.55.4",
        "axios": "^1.7.7",
        "clsx": "^2.1.1",
        "d3": "^7.8.5",
        "dompurify": "^3.0.9",
        "formik": "^2.4.6",
        "jwt-decode": "^4.0.0",
        "leaflet": "^1.9.4",
        "leaflet-draw": "^1.0.4",
        "lodash.clonedeep": "^4.5.0",
        "lodash.debounce": "^4.0.8",
        "lodash.groupby": "^4.6.0",
        "node-htmldiff": "^0.9.4",
        "proj4leaflet": "^1.0.2",
        "react": "^18.3.1",
        "react-content-loader": "^6.2.1",
        "react-dom": "^18.3.1",
        "react-error-boundary": "^4.0.13",
        "react-helmet-async": "^2.0.5",
        "react-leaflet": "^4.2.1",
        "react-router-dom": "^6.26.2",
        "uuid": "^10.0.0",
        "zod": "^3.23.8",
        "zod-formik-adapter": "^1.3.0",
        "zustand": "^5.0.0"
    },
    "scripts": {
        "build:css": "npx tailwindcss -i node_modules/@pzh-ui/css/src/tailwind.src.css -o src/css/tailwind.css",
        "watch:css": "npx tailwindcss -i node_modules/@pzh-ui/css/src/tailwind.src.css -o src/css/tailwind.css --watch",
        "start": "npm run-script build:css && concurrently \"npm run-script watch:css\" \"vite\"",
        "build": "npm run-script build:css --minify && tsc && vite build",
        "serve": "vite preview",
        "format": "prettier --write './src/**/*'",
        "test": "vitest watch",
        "test:no-watch": "vitest run",
        "test:coverage": "vitest run --coverage",
        "generate-types": "orval && eslint --fix './src/api/fetchers.ts'"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@axe-core/react": "^4.10.0",
        "@faker-js/faker": "^8.3.1",
        "@testing-library/dom": "^9.3.3",
        "@testing-library/jest-dom": "^6.1.5",
        "@testing-library/react": "^14.1.2",
        "@testing-library/user-event": "^14.5.1",
        "@total-typescript/ts-reset": "^0.5.1",
        "@types/d3": "^7.4.3",
        "@types/dompurify": "^3.0.2",
        "@types/leaflet": "^1.9.8",
        "@types/leaflet-draw": "^1.0.11",
        "@types/lodash.clonedeep": "^4.5.6",
        "@types/lodash.debounce": "^4.0.7",
        "@types/lodash.groupby": "^4.6.7",
        "@types/proj4leaflet": "^1.0.10",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@types/react-leaflet": "^3.0.0",
        "@types/react-router-dom": "^5.3.3",
        "@types/uuid": "^9.0.3",
        "@vitejs/plugin-react": "^4.3.1",
        "@vitest/coverage-v8": "^2.1.1",
        "add": "^2.0.6",
        "autoprefixer": "^10.4.4",
        "c8": "^8.0.0",
        "concurrently": "^8.2.2",
        "dotenv": "^16.4.5",
        "eslint": "^8.48.0",
        "eslint-plugin-prettier": "^5.0.0",
        "jsdom": "^25.0.1",
        "msw": "^2.4.9",
        "orval": "^6.25.0",
        "prettier": "2.8.8",
        "rollup-plugin-visualizer": "^5.11.0",
        "sass": "^1.63.6",
        "tailwindcss": "^3.4.1",
        "typescript": "^5.6.2",
        "vite": "^5.4.7",
        "vite-plugin-svgr": "^4.2.0",
        "vite-tsconfig-paths": "^5.0.1",
        "vitest": "^2.1.1"
    },
    "resolutions": {
        "prosemirror-model": "^1.22.2"
    },
    "msw": {
        "workerDirectory": "public"
    },
    "packageManager": "yarn@4.0.1",
    "prettier": "@pzh-ui/config/prettier"
}
