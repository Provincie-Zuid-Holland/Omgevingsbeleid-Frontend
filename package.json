{
    "name": "pzh",
    "version": "0.1.0",
    "private": true,
    "homepage": "https://obzhtest.azureedge.net/",
    "dependencies": {
        "@craco/craco": "7.0.0-alpha.3",
        "@headlessui/react": "^1.5.0",
        "@pzh-ui/components": "^0.0.196",
        "@pzh-ui/config": "^0.0.34",
        "@pzh-ui/css": "^0.0.49",
        "@pzh-ui/icons": "^0.0.13",
        "@sentry/browser": "^6.11.0",
        "@tanstack/react-query": "^4.13.0",
        "@tippyjs/react": "^4.2.5",
        "axios": "^0.24.0",
        "classnames": "^2.3.1",
        "d3": "^7.0.1",
        "date-fns": "^2.23.0",
        "dompurify": "^2.3.1",
        "formik": "^2.2.9",
        "framer-motion": "^6.3.3",
        "leaflet": "^1.7.1",
        "leaflet-draw": "^1.0.4",
        "lodash.clonedeep": "^4.5.0",
        "node-sass": "^7.0.1",
        "proj4leaflet": "^1.0.2",
        "promise-polyfill": "^8.2.0",
        "quill": "^1.3.7",
        "react": "^18.0.0",
        "react-app-polyfill": "^2.0.0",
        "react-beautiful-dnd": "^13.1.0",
        "react-content-loader": "^6.0.3",
        "react-dom": "^18.0.0",
        "react-error-boundary": "^3.1.4",
        "react-helmet": "^6.1.0",
        "react-leaflet": "^3.2.5",
        "react-router-dom": "^6.3.0",
        "react-scripts": "^5.0.0",
        "react-select": "^5.2.2",
        "react-toastify": "^7.0.4",
        "react-use": "^17.3.2",
        "smoothscroll-polyfill": "^0.4.4",
        "typescript": "4.4.4",
        "url-search-params-polyfill": "^8.1.1",
        "yup": "v1.0.0-beta.7",
        "zustand": "^4.1.1"
    },
    "scripts": {
        "build:css": "npx tailwindcss -i node_modules/@pzh-ui/css/src/tailwind.src.css -o src/css/tailwind.css",
        "watch:css": "npx tailwindcss -i node_modules/@pzh-ui/css/src/tailwind.src.css -o src/css/tailwind.css --watch",
        "prestart": "npm run-script build:css",
        "prebuild": "npm run-script build:css --minify",
        "start": "concurrently \"npm run-script watch:css\" \"craco start\"",
        "build": "craco build",
        "format": "prettier --write './src/**/*'",
        "prod:build-osx": "NODE_ENV=production GENERATE_SOURCEMAP=false npm run build",
        "prod:build-win": "SET NODE_ENV=production && SET GENERATE_SOURCEMAP=false && npm run build",
        "test:local": "craco test --watchAll --maxWorkers=25 --coverage",
        "test:ci": "craco test --watchAll=false --maxWorkers=2 --coverage --ci --forceExit",
        "eject": "react-scripts eject",
        "docs": "jsdoc -c jsdoc.json",
        "generate-types": "orval && eslint --fix './src/api/fetchers.ts'"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "ie 11",
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@axe-core/react": "4.4.1",
        "@testing-library/dom": "^8.13.0",
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^13.0.0",
        "@testing-library/user-event": "^14.0.4",
        "@types/d3": "^7.1.0",
        "@types/dompurify": "^2.3.3",
        "@types/jest": "^27.0.3",
        "@types/leaflet": "^1.7.8",
        "@types/leaflet-draw": "^1.0.5",
        "@types/lodash.clonedeep": "^4.5.6",
        "@types/node": "^17.0.0",
        "@types/proj4leaflet": "^1.0.6",
        "@types/quill": "^2.0.9",
        "@types/react": "^17.0.37",
        "@types/react-dom": "^18.0.0",
        "@types/react-helmet": "^6.1.5",
        "@types/react-leaflet": "^2.8.2",
        "@types/react-lines-ellipsis": "^0.15.1",
        "@types/react-router-dom": "^5.3.2",
        "@types/react-select": "^5.0.1",
        "@types/smoothscroll-polyfill": "^0.3.1",
        "@typescript-eslint/eslint-plugin": "^4.27.0",
        "@typescript-eslint/parser": "^4.27.0",
        "autoprefixer": "^10.4.4",
        "concurrently": "^7.2.1",
        "craco-alias": "^3.0.1",
        "eslint": "^7.28.0",
        "eslint-plugin-prettier": "^4.0.0",
        "msw": "^0.35.0",
        "orval": "^6.9.6",
        "prettier": "^2.3.1",
        "tailwindcss": "^3.0.23"
    },
    "msw": {
        "workerDirectory": "public"
    },
    "jest": {
        "resetMocks": true,
        "restoreMocks": true,
        "coveragePathIgnorePatterns": [
            "src/api/fetchers.ts",
            "src/api/fetchers.schemas.ts"
        ],
        "transformIgnorePatterns": [
            "node_modules/(?!d3)/"
        ],
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/mocks/fileMock.js",
            "\\.(css|less)$": "<rootDir>/src/mocks/styleMock.js"
        }
    },
    "resolutions": {
        "@types/react": "^17.0.37"
    }
}
