    "scripts": {
        "build:css": "npx tailwindcss -i node_modules/@pzh-ui/css/src/tailwind.src.css -o src/css/tailwind.css",
        "watch:css": "npx tailwindcss -i node_modules/@pzh-ui/css/src/tailwind.src.css -o src/css/tailwind.css --watch",
        "prestart": "npm run-script build:css",
        "prebuild": "npm run-script build:css --minify",
        "start": "craco start",
        "build": "craco build",
        "watch": "npm run-script watch:css",
        "format": "prettier --write './src/**/*'",
        "prod:build-osx": "NODE_ENV=production GENERATE_SOURCEMAP=false npm run build",
        "prod:build-win": "SET NODE_ENV=production && SET GENERATE_SOURCEMAP=false&& npm run build",
        "test:local": "craco test --watchAll --maxWorkers=25% --coverage",
        "test:ci": "craco test --watchAll=false --maxWorkers=2 --coverage --ci --forceExit",
        "eject": "react-scripts eject",
        "docs": "jsdoc -c jsdoc.json",
        "generate-types": "orval && eslint --fix './src/api/fetchers.ts'"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "ie 11",
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@axe-core/react": "4.4.1",
        "@babel/core": "^7.15.0",
        "@babel/eslint-parser": "^7.17.0",
        "@babel/preset-env": "^7.15.0",
