        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^12.1.2",
        "@testing-library/user-event": "^13.5.0",
        "@types/d3": "^7.1.0",
        "@types/dompurify": "^2.3.3",
        "@types/leaflet": "^1.7.8",
        "@types/leaflet-draw": "^1.0.5",
        "@types/lodash.clonedeep": "^4.5.6",
        "@types/lodash.isequal": "^4.5.5",
        "@types/proj4leaflet": "^1.0.6",
        "@types/quill": "^2.0.9",
        "@types/react-helmet": "^6.1.5",
        "@types/react-leaflet": "^2.8.2",
        "@types/react-router-dom": "^5.3.2",
        "@types/react-select": "^5.0.1",
        "@types/smoothscroll-polyfill": "^0.3.1",
        "@typescript-eslint/eslint-plugin": "^4.27.0",
        "@typescript-eslint/parser": "^4.27.0",
        "autoprefixer": "^10.4.4",
        "babel-eslint": "^10.1.0",
        "better-docs": "^2.3.2",
        "craco-alias": "^3.0.1",
        "eslint": "^7.28.0",
        "eslint-config-next": "^11.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-import-resolver-typescript": "^2.4.0",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-react": "^7.22.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "msw": "^0.35.0",
        "orval": "6.5.3",
        "prettier": "^2.3.1",
        "react-error-overlay": "6.0.9",
        "tailwindcss": "^3.0.23"
    },
    "msw": {
        "workerDirectory": "public"
    },
    "jest": {
        "resetMocks": true,
        "restoreMocks": true,
        "coveragePathIgnorePatterns": [
            "src/api/fetchers.ts",
            "src/api/fetchers.schemas.ts"
        ],
        "transformIgnorePatterns": [
            "node_modules/(?!d3)/"
        ],
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/mocks/fileMock.js",
            "\\.(css|less)$": "<rootDir>/src/mocks/styleMock.js"
        }
    },
    "resolutions": {
        "//": "See https://github.com/facebook/create-react-app/issues/11773",
        "react-error-overlay": "6.0.9"
    }
