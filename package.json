        "date-fns": "^2.23.0",
        "dompurify": "^3.0.5",
        "formik": "^2.4.2",
        "jwt-decode": "^3.1.2",
        "leaflet": "^1.7.1",
        "leaflet-draw": "^1.0.4",
        "lodash.clonedeep": "^4.5.0",
        "lodash.debounce": "^4.0.8",
        "lodash.groupby": "^4.6.0",
        "node-htmldiff": "^0.9.4",
        "node-sass": "9.0.0",
