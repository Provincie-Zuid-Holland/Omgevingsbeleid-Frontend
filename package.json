{
    "name": "pzh",
    "version": "0.1.0",
    "private": true,
    "homepage": "https://obzhtest.azureedge.net/",
    "dependencies": {
        "@headlessui/react": "^1.5.0",
        "@pzh-ui/components": "^0.0.352",
        "@pzh-ui/config": "^0.0.34",
        "@pzh-ui/css": "^0.0.64",
        "@pzh-ui/icons": "^0.0.31",
        "@sentry/browser": "^6.11.0",
        "@tanstack/react-query": "^4.29.5",
        "@tippyjs/react": "^4.2.5",
        "axios": "^0.24.0",
        "classnames": "^2.3.1",
        "d3": "^7.0.1",
        "d3-symbol-extra": "^0.1.0",
        "date-fns": "^2.23.0",
        "dompurify": "^2.3.1",
        "formik": "^2.2.9",
        "framer-motion": "^6.3.3",
        "jwt-decode": "^3.1.2",
        "leaflet": "^1.7.1",
        "leaflet-draw": "^1.0.4",
        "lodash.clonedeep": "^4.5.0",
        "lodash.debounce": "^4.0.8",
        "lodash.groupby": "^4.6.0",
        "node-htmldiff": "^0.9.4",
        "node-sass": "^7.0.1",
        "proj4leaflet": "^1.0.2",
        "promise-polyfill": "^8.2.0",
        "react": "^18.0.0",
        "react-accessible-accordion": "^5.0.0",
        "react-content-loader": "^6.0.3",
        "react-dom": "^18.0.0",
        "react-error-boundary": "^3.1.4",
        "react-helmet": "^6.1.0",
        "react-leaflet": "^3.2.5",
        "react-router-dom": "^6.9.0",
        "react-select": "^5.7.3",
        "react-use": "^17.3.2",
        "smoothscroll-polyfill": "^0.4.4",
        "typescript": "4.4.4",
        "url-search-params-polyfill": "^8.1.1",
        "zod": "^3.21.4",
        "zod-formik-adapter": "^1.2.0",
        "zustand": "^4.3.7"
    },
    "scripts": {
        "analyze": "source-map-explorer 'build/static/js/*.js'",
        "build:css": "npx tailwindcss -i node_modules/@pzh-ui/css/src/tailwind.src.css -o src/css/tailwind.css",
        "watch:css": "npx tailwindcss -i node_modules/@pzh-ui/css/src/tailwind.src.css -o src/css/tailwind.css --watch",
        "prestart": "npm run-script build:css",
        "prebuild": "npm run-script build:css --minify",
        "start": "concurrently \"npm run-script watch:css\" \"vite\"",
        "build": "tsc && vite build",
        "serve": "vite preview",
        "format": "prettier --write './src/**/*'",
        "test": "vitest watch",
        "test:no-watch": "vitest run",
        "test:coverage": "vitest run --coverage",
        "eject": "react-scripts eject",
        "generate-types": "orval && eslint --fix './src/api/fetchers.ts'"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "ie 11",
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@axe-core/react": "4.4.1",
        "@faker-js/faker": "^7.6.0",
        "@testing-library/dom": "^9.3.1",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^14.0.0",
        "@testing-library/user-event": "^14.4.3",
        "@total-typescript/ts-reset": "^0.4.2",
        "@types/d3": "^7.4.0",
        "@types/dompurify": "^2.3.3",
        "@types/jest": "^27.0.3",
        "@types/leaflet": "^1.7.8",
        "@types/leaflet-draw": "^1.0.5",
        "@types/lodash.clonedeep": "^4.5.6",
        "@types/lodash.debounce": "^4.0.7",
        "@types/lodash.groupby": "^4.6.7",
        "@types/node": "^17.0.0",
        "@types/proj4leaflet": "^1.0.6",
        "@types/react": "^17.0.37",
        "@types/react-dom": "^18.0.0",
        "@types/react-helmet": "^6.1.5",
        "@types/react-leaflet": "^2.8.2",
        "@types/react-lines-ellipsis": "^0.15.1",
        "@types/react-router-dom": "^5.3.2",
        "@types/react-select": "^5.0.1",
        "@types/smoothscroll-polyfill": "^0.3.1",
        "@typescript-eslint/eslint-plugin": "^4.27.0",
        "@typescript-eslint/parser": "^4.27.0",
        "@vitejs/plugin-react": "^4.0.1",
        "add": "^2.0.6",
        "autoprefixer": "^10.4.4",
        "c8": "^8.0.0",
        "concurrently": "^7.2.1",
        "dotenv": "^16.3.1",
        "eslint": "^7.28.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jsdom": "^22.1.0",
        "msw": "^0.35.0",
        "orval": "^6.16.0",
        "prettier": "^2.3.1",
        "rollup-plugin-visualizer": "^5.9.2",
        "sass": "^1.63.6",
        "source-map-explorer": "^2.5.3",
        "tailwindcss": "^3.2.4",
        "vite": "^4.3.9",
        "vite-plugin-svgr": "^3.2.0",
        "vite-tsconfig-paths": "^4.2.0",
        "vitest": "^0.32.2",
        "yarn": "^1.22.19"
    },
    "msw": {
        "workerDirectory": "public"
    },
    "jest": {
        "resetMocks": true,
        "restoreMocks": true,
        "coveragePathIgnorePatterns": [
            "src/api/fetchers.ts",
            "src/api/fetchers.schemas.ts"
        ],
        "transformIgnorePatterns": [
            "node_modules/(?!d3)/"
        ],
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/mocks/fileMock.js",
            "\\.(css|less)$": "<rootDir>/src/mocks/styleMock.js"
        }
    },
    "resolutions": {
        "@types/react": "^17.0.37"
    }
}
