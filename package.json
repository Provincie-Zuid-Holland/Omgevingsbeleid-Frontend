        "zod": "^3.21.4",
        "zod-formik-adapter": "^1.2.0",
        "zustand": "^4.3.9"
    },
    "scripts": {
        "build:css": "npx tailwindcss -i node_modules/@pzh-ui/css/src/tailwind.src.css -o src/css/tailwind.css",
        "watch:css": "npx tailwindcss -i node_modules/@pzh-ui/css/src/tailwind.src.css -o src/css/tailwind.css --watch",
        "start": "npm run-script build:css && concurrently \"npm run-script watch:css\" \"vite\"",
        "build": "npm run-script build:css --minify && tsc && vite build",
        "serve": "vite preview",
        "format": "prettier --write './src/**/*'",
        "test": "vitest watch",
        "test:no-watch": "vitest run",
        "test:coverage": "vitest run --coverage",
        "generate-types": "orval && eslint --fix './src/api/fetchers.ts'"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@axe-core/react": "4.4.1",
        "@faker-js/faker": "^7.6.0",
        "@testing-library/dom": "^9.3.1",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^14.0.0",
        "@testing-library/user-event": "^14.4.3",
        "@total-typescript/ts-reset": "^0.4.2",
        "@types/d3": "^7.4.0",
        "@types/dompurify": "^3.0.2",
        "@types/leaflet": "^1.7.8",
        "@types/leaflet-draw": "^1.0.5",
        "@types/lodash.clonedeep": "^4.5.6",
        "@types/lodash.debounce": "^4.0.7",
        "@types/lodash.groupby": "^4.6.7",
        "@types/node": "^17.0.0",
        "@types/proj4leaflet": "^1.0.6",
        "@types/react": "^18.2.15",
        "@types/react-dom": "^18.2.7",
        "@types/react-helmet": "^6.1.6",
        "@types/react-leaflet": "^2.8.2",
        "@types/react-router-dom": "^5.3.2",
        "@types/uuid": "^9.0.3",
        "@vitejs/plugin-react": "^4.0.3",
        "@vitest/coverage-v8": "^0.33.0",
        "add": "^2.0.6",
        "autoprefixer": "^10.4.4",
        "c8": "^8.0.0",
        "concurrently": "^7.2.1",
        "dotenv": "^16.3.1",
        "eslint": "^8.48.0",
        "eslint-plugin-prettier": "^5.0.0",
        "jsdom": "^22.1.0",
        "msw": "^0.35.0",
        "orval": "^6.16.0",
        "prettier": "2.8.8",
        "rollup-plugin-visualizer": "^5.9.2",
        "sass": "^1.63.6",
        "tailwindcss": "^3.2.4",
        "typescript": "^5.1.6",
        "vite": "^4.4.7",
        "vite-plugin-svgr": "^3.2.0",
        "vite-tsconfig-paths": "^4.2.0",
        "vitest": "^0.32.2"
    },
    "msw": {
        "workerDirectory": "public"
    },
    "packageManager": "yarn@4.0.1",
    "prettier": "@pzh-ui/config/prettier"
}
